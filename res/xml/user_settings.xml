<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    <PreferenceCategory android:title="@string/uisettings" >
    	<ListPreference
            android:key="prefDefaultCountry"
            android:title="Default Country"
            android:summary="Set Default Country"
            android:entries="@array/country"
            android:entryValues="@array/countryValues"
            />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/rtlsettings" >
    	<ListPreference
            android:key="prefDefaultFreq"
            android:title="Default Frequency"
            android:summary="Set Default Frequency"
            android:entries="@array/frequency_array"
            android:entryValues="@array/frequency_arrayValues"
            />
    </PreferenceCategory>

</PreferenceScreen>
